{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchDatasources": ["go"],
      "matchUpdateTypes": ["major", "minor", "patch"],
      "enabled": false
    },
    {
      "matchDatasources": ["go"],
      "matchUpdateTypes": ["major", "minor", "patch"],
      "matchVulnerabilityAlerts": true,
      "enabled": true
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}